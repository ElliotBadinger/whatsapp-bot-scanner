{
  "name": "whatsapp-bot-scanner",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.3",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "validate": "./scripts/validate-types.sh",
    "validate:quick": "bun run --filter '*' build && bun run --filter '*' test -- --passWithNoTests",
    "type-check": "bun run --filter '*' type-check",
    "type-check:strict": "tsc --noEmit --strict && bun run --filter '*' type-check",
    "lint": "bun run --filter '*' lint",
    "lint:fix": "bun run --filter '*' lint:fix",
    "test": "bun run --filter '*' test -- --passWithNoTests",
    "build": "bun run --filter '*' build",
    "clean": "bun run --filter '*' clean && rm -rf node_modules",
    "precommit": "./scripts/validate-types.sh"
  },
  "workspaces": [
    "packages/*",
    "services/*",
    "apps/*",
    "SafeMode-web-app"
  ],
  "dependencies": {
    "next": "16.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.2",
    "cli-table3": "^0.6.3",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "humanize-duration": "^3.31.0",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "log-symbols": "^6.0.0",
    "nanospinner": "^1.1.0",
    "ora": "^7.0.1",
    "ts-jest": "^29.1.2"
  },
  "overrides": {
    "tar-fs": "^3.1.1",
    "tmp": "^0.2.4",
    "tough-cookie": "^5.1.2",
    "ws": "^8.17.1",
    "form-data": "^4.0.2"
  },
  "resolutions": {
    "tar-fs": "3.1.1",
    "tmp": "0.2.4",
    "tough-cookie": "5.1.2",
    "ws": "8.17.1",
    "form-data": "4.0.2",
    "inflight": "1.0.6"
  }
}